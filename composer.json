{
    "name": "ymigval/laravel-wwwprotocolresolver",
    "type": "library",
    "description": "A Laravel package for redirecting responses with or without www and with the specified protocol (http or https).",
    "keywords": [
        "laravel",
        "www",
        "protocol",
        "redirect",
        "http",
        "https"
    ],
    "homepage": "https://github.com/ymigval/laravel-wwwprotocolresolver",
    "license": "MIT",
    "authors": [
        {
            "name": "Yordan",
            "email": "banatube@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^8.21",
        "laravel/pint": "^1.13"
    },
    "require": {
        "php": "^8.0",
        "spatie/url": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Ymigval\\LaravelWwwProtocolResolver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ymigval\\LaravelWwwProtocolResolver\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ymigval\\LaravelWwwProtocolResolver\\Providers\\WWWProtocolResolverProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}